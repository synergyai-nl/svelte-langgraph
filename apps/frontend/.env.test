# E2E Test Environment Configuration
# Using mock authentication for testing

# Test User Credentials (mock)
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=test_password_123

# Mock Authentication - bypasses real Descope
ENABLE_MOCK_AUTH=true

# Base URLs
BASE_URL=http://localhost:4173
PUBLIC_LANGGRAPH_API_URL=http://127.0.0.1:2024

# Authentication Configuration (required but not used with mock auth)
AUTH_DESCOPE_ID=mock_id
AUTH_DESCOPE_SECRET=mock_secret
AUTH_DESCOPE_ISSUER=https://api.descope.com
AUTH_SECRET=test_auth_secret_key_for_testing_only
AUTH_TRUST_HOST=true

# Test Execution Settings
HEADLESS=true
SLOW_MO=0
TEST_TIMEOUT=30000
TEST_RETRIES=1
TEST_WORKERS=2

# Test Environment
NODE_ENV=test
CI=false

# Feature Flags
ENABLE_VIDEO=false
ENABLE_TRACE=false
ENABLE_SCREENSHOT=true

# Artifact Management
CLEAN_ARTIFACTS=false
ARCHIVE_ARTIFACTS=false