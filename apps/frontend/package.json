{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest run",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.35.0",
		"@playwright/test": "^1.55.0",
		"@sveltejs/adapter-node": "^5.3.2",
		"@sveltejs/kit": "^2.41.0",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.13",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/svelte": "^5.2.8",
		"@vitest/coverage-v8": "^3.2.4",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.3",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.13.8",
		"flowbite-svelte-icons": "^2.3.0",
		"globals": "^16.4.0",
		"jsdom": "^26.1.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.38.10",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.0",
		"vite": "^6.3.6",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@auth/sveltekit": "^1.10.0",
		"@inlang/paraglide-js": "^2.3.2",
		"@langchain/core": "^0.3.76",
		"@langchain/langgraph-sdk": "^0.0.103",
		"@sentry/sveltekit": "^10.11.0",
		"flowbite-svelte-blocks": "^2.1.0",
		"svelte-exmarkdown": "^5.0.2"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
